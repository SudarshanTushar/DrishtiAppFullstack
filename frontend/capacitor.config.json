{
  "appId": "com.drishti.ne",
  "appName": "DrishtiNE",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "launchFadeOutDuration": 500,
      "backgroundColor": "#020617",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "TestPlugin": {
      "enabled": true
    },
    "MeshNetwork": {
      "enabled": true
    }
  }
}
